exercico 6 
def eh_ano_bissexto(ano):
    if ano % 4 == 0:
        if ano % 100 == 0:
            if ano % 400 == 0:
                return True
            else:
                return False
        else:
            return True
    else:
        return False

# Exemplo de uso da função
ano = int(input("Digite um ano: "))

if eh_ano_bissexto(ano):
    print(ano, "é um ano bissexto.")
else:
    print(ano, "não é um ano bissexto.")
   
   
exercicio 3
def cadastrar_funcionario(nome, salario=9000):
    print("Nome do funcionário:", nome)
    print("Salário do funcionário:", salario)

# Exemplo de uso da função
nome = input("Digite o nome do funcionário: ")
salario = float(input("Digite o salário do funcionário (opcional): ") or 9000)

cadastrar_funcionario(nome, salario)
